{
  "name": "ember-cli-deploy-gzip",
  "version": "1.0.1",
  "description": "Ember CLI Deploy plugin to gzip files.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js tests/**/*.js"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-gzip",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Luke Melia and ember-cli-deploy team",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.22.0",
    "eslint": "^7.14.0",
    "github": "^14.0.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "multiline": "^2.0.0",
    "release-it": "14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.6",
    "minimatch": "^3.0.4",
    "rsvp": "^4.8.5"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  }
}
